<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="NewApi">
    <aapt:attr name="android:drawable">
        <vector
            android:width="32dp"
            android:height="32dp"
            android:viewportWidth="400"
            android:viewportHeight="400">
            <group
                android:name="one"
                android:pivotX="200"
                android:pivotY="200">
                <path
                    android:fillAlpha="0"
                    android:fillColor="@color/iconInApp"
                    android:pathData="M25,250Z"
                    android:strokeWidth="50"
                    android:strokeAlpha="1"
                    android:strokeColor="@color/iconInApp"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round" />
            </group>
            <group
                android:name="two"
                android:pivotX="200"
                android:pivotY="200">
                <path
                    android:fillAlpha="0"
                    android:fillColor="@color/iconInApp"
                    android:pathData="M200,250Z"
                    android:strokeWidth="50"
                    android:strokeAlpha="1"
                    android:strokeColor="@color/iconInApp"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round" />
            </group>
            <group
                android:name="three"
                android:pivotX="200"
                android:pivotY="200">
                <path
                    android:fillAlpha="0"
                    android:fillColor="@color/iconInApp"
                    android:pathData="M375,250Z"
                    android:strokeWidth="50"
                    android:strokeAlpha="1"
                    android:strokeColor="@color/iconInApp"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round" />
            </group>
        </vector>
    </aapt:attr>
    <target android:name="one">
        <aapt:attr name="android:animation">
                <objectAnimator
                    android:duration="350"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="translateY"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:valueFrom="0"
                    android:valueTo="-150" />
        </aapt:attr>
    </target>
    <target android:name="two">
        <aapt:attr name="android:animation">
                <objectAnimator
                    android:duration="350"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="translateY"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:valueFrom="0"
                    android:valueTo="-150" />
        </aapt:attr>
    </target>
    <target android:name="three">
        <aapt:attr name="android:animation">
                <objectAnimator
                    android:duration="350"
                    android:interpolator="@android:anim/linear_interpolator"
                    android:propertyName="translateY"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:valueFrom="0"
                    android:valueTo="-150" />
        </aapt:attr>
    </target>
</animated-vector>
