<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerControls">
        <attr name="progressAnimDuration" format="integer"/>
        <attr name="changeTrackAnimDuration" format="integer"/>
        <attr name="smallestButtonWidth" format="dimension"/>
        <attr name="currentDuration" format="integer"/>
        <attr name="totalDuration" format="integer"/>
        <attr name="state" format="enum">
            <enum name="play" value="0"/>
            <enum name="pause" value="1"/>
            <enum name="buffer" value="2"/>
        </attr>
    </declare-styleable>
</resources>